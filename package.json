{
  "name": "pong420",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "lint": "eslint 'src/**/*.ts?(x)'",
    "component": "node scripts/component.js",
    "page": "node scripts/page.js",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --rule 'no-console: [\"error\", { allow: [\"warn\", \"error\"] }]'  --max-warnings=0",
      "prettier --ignore-path .eslintignore --write"
    ],
    "{*.json,.{babelrc,eslintrc,prettierrc}}": [
      "prettier --ignore-path .eslintignore --write"
    ],
    "*.{css,scss}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ],
    "*.{yml,md}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "typeface-open-sans": "^1.1.13",
    "typeface-raleway": "^1.1.13",
    "typeface-roboto": "^1.1.13",
    "video.js": "7.14.3",
    "videojs-plus": "^1.6.9"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.172",
    "@types/node": "^16.7.10",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/react-helmet": "^6.1.2",
    "@types/video.js": "^7.3.11",
    "eslint-config-react-app": "^6.0.0",
    "gatsby": "^3.13.0",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-manifest": "^3.13.0",
    "gatsby-plugin-offline": "^4.13.0",
    "gatsby-plugin-react-helmet": "^4.13.0",
    "gatsby-plugin-react-svg": "^3.0.1",
    "gatsby-plugin-resolve-src": "^2.1.0",
    "gatsby-plugin-sass": "^4.13.0",
    "gatsby-plugin-sharp": "^3.13.0",
    "gatsby-plugin-typescript": "^3.13.0",
    "gatsby-remark-images": "^5.10.0",
    "gatsby-source-filesystem": "^3.13.0",
    "gatsby-transformer-remark": "^5.25.1",
    "gatsby-transformer-sharp": "^3.13.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "sass": "^1.39.0",
    "typescript": "^4.4.2",
    "webpack": "^5.52.0"
  }
