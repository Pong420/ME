{
  "name": "pong",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev": "yarn projects:dev & gatsby develop",
    "serve": "gatsby serve",
    "projects": "node scripts/projects.js",
    "projects:dev": "nodemon -e html,json --watch projects --exec \"yarn projects\"",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "eslint": "eslint 'src/**/*.ts?(x)'",
    "component": "node scripts/component.js",
    "page": "node scripts/page.js",
    "get": "node scripts/get.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint",
      "git add"
    ]
  },
  "dependencies": {
    "lodash.random": "^3.2.0",
    "object-fit-images": "^3.2.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "typeface-open-sans": "^0.0.75",
    "typeface-raleway": "^0.0.75",
    "typeface-roboto": "^0.0.75",
    "video.js": "^7.6.5",
    "videojs-plus": "^1.5.7"
  },
  "devDependencies": {
    "@types/lodash.random": "^3.2.6",
    "@types/node": "^12.12.20",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^5.0.14",
    "@types/video.js": "^7.3.3",
    "eslint-config-react-app": "^5.1.0",
    "gatsby": "^2.18.13",
    "gatsby-image": "^2.2.37",
    "gatsby-plugin-manifest": "^2.2.33",
    "gatsby-plugin-offline": "^3.0.29",
    "gatsby-plugin-react-helmet": "^3.1.18",
    "gatsby-plugin-resolve-src": "^2.0.0",
    "gatsby-plugin-sass": "^2.1.24",
    "gatsby-plugin-sharp": "^2.3.9",
    "gatsby-plugin-typescript": "^2.1.23",
    "gatsby-source-filesystem": "^2.1.42",
    "gatsby-transformer-sharp": "^2.3.9",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "nodemon": "^2.0.2",
    "sass": "^1.23.7",
    "typescript": "3.7.3",
    "webpack": "^4.41.3"
  }
}
