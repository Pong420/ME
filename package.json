{
  "name": "pong",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev": "yarn projects:dev & gatsby develop",
    "serve": "gatsby serve",
    "projects": "node scripts/projects.js",
    "projects:dev": "nodemon -e html,json --watch projects --exec \"yarn projects\"",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "tslint": "tslint 'src/**/*.ts?(x)'",
    "component": "node scripts/component.js",
    "page": "node scripts/page.js",
    "get": "node scripts/get.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn tslint",
      "git add"
    ]
  },
  "dependencies": {
    "lodash.random": "^3.2.0",
    "object-fit-images": "^3.2.4",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "typeface-open-sans": "^0.0.75",
    "typeface-raleway": "^0.0.75",
    "typeface-roboto": "^0.0.75",
    "video.js": "^7.6.0",
    "videojs-plus": "^1.5.4"
  },
  "devDependencies": {
    "@types/lodash.random": "^3.2.6",
    "@types/node": "^12.7.1",
    "@types/react": "^16.9.1",
    "@types/react-dom": "^16.8.5",
    "@types/react-helmet": "^5.0.9",
    "@types/video.js": "^7.2.11",
    "eslint-config-react-app": "^5.0.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "gatsby": "^2.13.59",
    "gatsby-image": "^2.2.8",
    "gatsby-plugin-manifest": "^2.2.5",
    "gatsby-plugin-offline": "^2.2.5",
    "gatsby-plugin-react-helmet": "^3.1.3",
    "gatsby-plugin-resolve-src": "^2.0.0",
    "gatsby-plugin-sass": "^2.1.8",
    "gatsby-plugin-sharp": "^2.2.11",
    "gatsby-plugin-tslint": "^0.0.2",
    "gatsby-plugin-typescript": "^2.1.2",
    "gatsby-source-filesystem": "^2.1.9",
    "gatsby-transformer-sharp": "^2.2.6",
    "husky": "^3.0.3",
    "lint-staged": "^9.2.1",
    "nodemon": "^1.19.1",
    "sass": "^1.22.9",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "tslint-react": "^4.0.0",
    "typescript": "3.5.3",
