{
  "name": "gatsby-starter-typescript",
  "version": "1.0.0",
  "private": true,
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "lint": "eslint 'src/**/*.ts?(x)'",
    "precommit": "lint-staged",
    "component": "node scripts/component.js",
    "page": "node scripts/page.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --rule 'no-console: [\"error\", { allow: [\"warn\", \"error\"] }]'  --max-warnings=0",
      "prettier --ignore-path .eslintignore --write",
      "git add"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "lodash.random": "^3.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "typeface-open-sans": "^0.0.75",
    "typeface-raleway": "^0.0.75",
    "typeface-roboto": "^0.0.75"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.5.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.15",
    "eslint-config-react-app": "^5.1.0",
    "gatsby": "^2.19.5",
    "gatsby-image": "^2.2.39",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-resolve-src": "^2.0.0",
    "gatsby-plugin-sass": "^2.1.27",
    "gatsby-plugin-sharp": "^2.4.0",
    "gatsby-plugin-typescript": "^2.1.26",
    "gatsby-source-filesystem": "^2.1.46",
    "gatsby-transformer-sharp": "^2.3.13",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.2",
    "sass": "^1.24.4",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5"
  }
}
